<xsl:output method="html" />
<xsl:template match="/NewDataSet/Table">
<xsl:choose>
<xsl:when test="pe:GetNodeFieldName(NodeID,'ItemPageSize')=0">
10
</xsl:when>
<xsl:otherwise>
<xsl:value-of select="pe:GetNodeFieldName(NodeID,'ItemPageSize')"/>
</xsl:otherwise>
